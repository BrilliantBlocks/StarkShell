{
    "image": "",
    "name": "zkode DApp NFT",
    "description": "Ownership of smart contract with address equalling token id - powered by BrilliantBlocks https://brilliantblocks.io",
    "properties": [
        { "dapp_info": "url", "value": "" },
        { "dapp_info": "name", "value": "" },
        { "dapp_info": "description", "value": "" },
        { "dapp_info": "logo", "value": "" }
    ],
    "updatable": {
        "engine": "jsonata@v1.8.*",
        "recipes": {
            "updateURL": "$ ~> | properties[dapp_info='url'] | 'value': $...",
            "updateName": "$ ~> | properties[dapp_info='name'] | 'value': $...",
            "updateDescription": "$ ~> | properties[dapp_info='description'] | 'value': $...",
            "updateLogo": "$ ~> | properties[dapp_info='logo'] | 'value': $..."
        }
    }
}
